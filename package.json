{
  "name": "testes-e2e-com-cypress",
  "version": "1.0.0",
  "description": "Sample project to demonstrate e2e tests written with Cypress running on GitHub Actions",
  "main": "index.js",
  "scripts": {
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix",
    "cy:open": "cypress open",
    "cy:open:tablet": "cypress open --config viewportWidth=767,viewportHeight=480",
    "test": "cypress run --record --tag 'desktop'",
    "test:tablet": "cypress run --record --tag 'tablet' --config viewportWidth=767,viewportHeight=480 --spec cypress/e2e/authenticatedScenarios.spec.js --env grepTags=@desktop-and-tablet"
  },
  "keywords": [],
  "author": "Walmyr Filho <wlsf82@gmail.com> (https://walmyr.dev)",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "cypress": "^10.10.0",
    "cypress-grep": "^3.0.3",
    "cypress-iframe": "^1.0.1",
    "cypress-mailosaur": "^2.3.3",
    "eslint": "^8.25.0",
    "eslint-plugin-cypress": "^2.12.1"
  }
}
